#pragma GCC system_header

#ifndef _LIBCXX_CSTDIO
#define _LIBCXX_CSTDIO

#include <__config>
#include <stddef.h>
#include <stdio.h>

_LIBCXX_BEGIN_NAMESPACE_STD

using ::fclose;
using ::FILE;
using ::fopen;

using ::setbuf;
using ::setlinebuf;
using ::setvbuf;

using ::fprintf;
using ::printf;
using ::snprintf;
using ::sprintf;
using ::vsnprintf;
using ::vsprintf;

using ::scanf;
using ::sscanf;
using ::vsscanf;

_LIBCXX_END_NAMESPACE_STD

#endif // _LIBCXX_CSTDIO